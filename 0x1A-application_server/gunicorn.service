[Unit]
 Description=Gunicorn instance to serve hnb web dynamic
 After=network.target

[Service]
 User=ubuntu
 Group=www-data
 workingDirectory=/home/ubuntu/AirBnB_clone_v4/web_dynamic/2-hbnb.py
 Environment="PATH=/home/ubuntu/AirBnB_clone_v4/venv/bin"
 ExecStart=/home/ubuntu/AirBnB_clone_v4/venv/bin/gunicorn --workers 3 --bind unix:AirBnB_clone_v4.sock -m 007 web_dynamic.2-hbnb:app

[Install]
WantedBy=multi-user.target
